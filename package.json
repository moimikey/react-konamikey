{
  "name": "react-konamikey",
  "version": "1.0.2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn cleanup && yarn compile",
    "cleanup": "rimraf index.js",
    "compile": "babel src/index.js -o index.js",
    "flow": "flow --one-line --show-all-errors",
    "flow:typed": "flow-typed",
    "lint": "standard src",
    "prebuild": "yarn lint && yarn test:ci && yarn flow",
    "prepare": "yarn build",
    "test": "jest",
    "test:ci": "jest --verbose --runInBand --ci"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8",
    "babel-jest": "^22.0.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-plugin-flow": "^2.29.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-react": "^7.5.1",
    "extract-react-types": "^0.4.2",
    "flow-bin": "^0.61.0",
    "flow-typed": "^2.2.3",
    "jest": "^22.0.3",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "jest": {
    "setupFiles": [
      "./.setup",
      "raf/polyfill"
    ]
  },
  "standard": {
    "globals": [],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype",
      "react"
    ],
    "env": [
      "browser",
      "jest",
      "es6",
      "shared-node-browser"
    ]
  },
  "description": "💅🏻 trigger a callback function on anything with a sequence of key presses",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moimikey/react-konamikey.git"
  },
  "keywords": [
    "react",
    "konami",
    "keydown",
    "keypress",
    "sequence",
    "easteregg"
  ],
  "author": "Michael Scott Hertzberg <mshertzberg@gmail.com>",
  "bugs": {
    "url": "https://github.com/moimikey/react-konamikey/issues"
  },
  "homepage": "https://github.com/moimikey/react-konamikey#readme"
}
