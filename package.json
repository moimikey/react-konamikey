    "konami",
    "keydown",
    "keypress",
    "sequence",
    "easteregg"
  ],
  "author": "Michael Scott Hertzberg <mshertzberg@gmail.com>",
  "bugs": {
    "url": "https://github.com/moimikey/react-konamikey/issues"
